{
  "extends": "eslint:recommended",
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018,
    "ecmaFeatures": {
        "globalReturn": false,
        "impliedStrict": false,
        "jsx": false
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off"
  }
}
